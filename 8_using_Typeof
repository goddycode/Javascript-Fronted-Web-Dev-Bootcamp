/*
Checkining datatypes in Javascripts
*/

const variable1 = 20;
const variable2 = "some value";
const variable3 = false; 

console.log("The data type of 20 " +typeof variable1);

console.log("The data type of 'some value' " +typeof variable2);

console.log("The data type of 'false'  " +typeof variable3); 
